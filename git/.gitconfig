[user]
	name = 
	email = 
[github]
	user = 
	token = 
	password = 
[core]
	excludesfile = ~/.gitignore_global
[color]
	ui = true
	diff = true
[color "diff"]
	meta = blue black bold
[alias]
  st = status -s
  ci = commit -s
  ciaa = commit -a --amend
  co = checkout
  br = branch
  df = diff
  dfp = diff -p
  dfs = diff --stat
  dfps = diff -p --stat
  ft = fetch -v
  sub = reset HEAD --
  fta = fetch --all -v
  l = log --graph --decorate --oneline --all
  lp = log --graph --decorate -p
  lop = log --graph --decorate --oneline -p
  ls = log --graph --decorate --oneline --all --stat
  lsp = log --graph --decorate --all --stat -p
  lo = log --graph --decorate --oneline
  ld = log --graph --all --pretty=format:'%C(yellow)%h%C(cyan)%d%Creset %s %C(white)- %C(yellow)%an, %C(cyan)%ar%Creset'
  ldo = log --graph --decorate --oneline --pretty=format:'%C(yellow)%h%C(cyan)%d%Creset %s %C(white)- %C(yellow)%an, %C(cyan)%ar%Creset'
  stash-unapply = !git stash show -p | git apply -R
[push]
	default = simple
[fetch]
  prune = true

[branch "development"]
    mergeoptions = --no-ff

